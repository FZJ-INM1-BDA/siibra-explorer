    "@storybook/manager-webpack5": "^6.4.22",
    "@storybook/testing-library": "0.0.11",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^4.29.2",
    "@typescript-eslint/parser": "^4.29.2",
    "babel-loader": "^8.2.3",
    "eslint": "^7.32.0",
