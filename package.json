    "@angular/cli": "^15.2.10",
    "@angular/compiler-cli": "^15.2.10",
    "@babel/core": "^7.17.5",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "babel-loader": "^8.2.3",
    "eslint": "^8.28.0",
